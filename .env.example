# CardioGenie Environment Variables
# Copy this file to .env and fill in your actual values

# AI Configuration
GROQ_API_KEY=your_groq_api_key_here

# Telegram Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
DOCTOR_CHAT_ID=your_doctor_chat_id_here

# Google Calendar Configuration
DOCTOR_EMAIL=your_doctor_email@gmail.com
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

# Database Configuration (optional)
DATABASE_PATH=database/cardiogenie_production.db

# Server Configuration (optional)
PORT=8000
DEBUG=false

# For Railway deployment, update GOOGLE_REDIRECT_URI to:
# GOOGLE_REDIRECT_URI=https://your-app-name.railway.app/auth/google/callback
